{
    "name": "nestjs-monolithic-boilerplate",
    "version": "1.0.0",
    "description": "NestJs, Typescript, Postgres, TypeORM",
    "author": "nestjs-monolithic-boilerplate",
    "private": true,
    "license": "MIT",
    "scripts": {
        "build:prod": "tsc -p tsconfig.build.json",
        "postbuild:prod": "copyfiles --up 1 src/**/*.json dist",
        "start:hmr": "node dist/main.hmr.ts",
        "start:dev": "ts-node src/main.ts",
        "watch": "ts-node-dev src/main.ts",
        "start": "node dist/main.js",
        "typeorm": "typeorm-ts-node-esm",
        "migration:generate": "yarn typeorm migration:generate -d ormconfig",
        "migration:run": "yarn typeorm migration:run -d ormconfig",
        "migration:revert": "yarn typeorm migration:revert -d ormconfig",
        "migration:create": "yarn typeorm migration:create",
        "seed": "ts-node ./node_modules/typeorm-extension/dist/cli/index.js seed -d ormconfig",
        "schema:drop": "yarn typeorm schema:drop -d ormconfig",
        "schema:sync": "yarn typeorm schema:sync -d ormconfig",
        "debug:dev": "cross-env TS_NODE_CACHE=false ts-node-dev --inspect --ignore '/^src/.*\\.spec\\.ts$/' src/main.ts",
        "webpack": "webpack --config webpack.config.js --progress",
        "new": "hygen new",
        "lint": "eslint --fix . --ext .ts",
        "test": "NODE_ENV=test jest",
        "test:watch": "NODE_ENV=test jest --watch",
        "test:cov": "NODE_ENV=test jest --coverage",
        "test:debug": "NODE_ENV=test node --inspect-brk -r ts-node/register node_modules/.bin/jest --runInBand",
        "copy:lib": "cpx 'src/lib/**/{files,workspace}/**/*.*' dist/lib && cpx 'src/lib/**/{files,workspace}/**/.!(gitignore)' dist/lib",
        "test:e2e": "NODE_ENV=test jest --config ./test/jest-e2e.json",
        "prepare": "husky install",
        "prerelease": "yarn docs:deploy",
        "release": "release-it",
        "pre-commit": "yarn lint-staged",
        "run-localtunnel": "npx localtunnel --subdomain nestjs-monolithic-boilerplate --port 4000"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.218.0",
        "@aws-sdk/client-ses": "^3.218.0",
        "@aws-sdk/s3-request-presigner": "^3.218.0",
        "@faker-js/faker": "^7.6.0",
        "@nestjs/axios": "^1.0.0",
        "@nestjs/common": "^9.2.1",
        "@nestjs/config": "^2.3.1",
        "@nestjs/core": "^9.2.1",
        "@nestjs/cqrs": "^9.0.1",
        "@nestjs/jwt": "^9.0.0",
        "@nestjs/mapped-types": "*",
        "@nestjs/passport": "^9.0.0",
        "@nestjs/platform-express": "^9.2.1",
        "@nestjs/platform-socket.io": "^9.4.0",
        "@nestjs/schedule": "^2.1.0",
        "@nestjs/swagger": "^6.1.3",
        "@nestjs/terminus": "^9.1.4",
        "@nestjs/typeorm": "^9.0.1",
        "@nestjs/websockets": "^9.4.0",
        "@types/nodemailer": "^6.4.6",
        "ably": "^1.2.39",
        "bcrypt": "^5.1.0",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.13.2",
        "compression": "^1.7.4",
        "exceljs": "^4.3.0",
        "express": "^4.18.2",
        "express-ctx": "^0.1.1",
        "express-rate-limit": "^6.7.0",
        "geoip-lite": "^1.4.6",
        "googleapis": "^109.0.1",
        "helmet": "^6.0.1",
        "jsonwebtoken": "^8.5.1",
        "lodash": "^4.17.21",
        "mime-types": "^2.1.35",
        "morgan": "^1.10.0",
        "nodemailer": "^6.8.0",
        "otplib": "^12.0.1",
        "passport": "^0.6.0",
        "passport-jwt": "^4.0.0",
        "pg": "^8.8.0",
        "pixelmatch": "^5.3.0",
        "pngjs": "^7.0.0",
        "puppeteer": "^20.5.0",
        "rand-token": "^1.0.1",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^7.5.7",
        "slugify": "^1.6.5",
        "swagger-ui-express": "^4.6.0",
        "typeorm": "^0.3.11",
        "typeorm-transactional-cls-hooked": "^0.1.21",
        "uuid": "^9.0.0",
        "verify-apple-id-token": "^3.0.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.3.0",
        "@commitlint/config-angular": "^17.3.0",
        "@commitlint/config-conventional": "^17.3.0",
        "@handfish/hygen": "^6.1.6",
        "@nestjs/cli": "^9.1.5",
        "@nestjs/testing": "^9.2.1",
        "@types/bcrypt": "^5.0.0",
        "@types/cache-manager-redis-store": "^2.0.1",
        "@types/compression": "^1.7.2",
        "@types/cron": "^2.0.0",
        "@types/express": "^4.17.14",
        "@types/geoip-lite": "^1.4.1",
        "@types/jest": "^29.2.3",
        "@types/jsonwebtoken": "^8.5.9",
        "@types/lodash": "^4.14.190",
        "@types/mime-types": "^2.1.1",
        "@types/morgan": "^1.9.3",
        "@types/multer": "^1.4.7",
        "@types/node": "^18.11.9",
        "@types/passport-jwt": "^3.0.7",
        "@types/supertest": "^2.0.12",
        "@types/uuid": "^9.0.0",
        "@types/webpack-env": "^1.18.0",
        "@typescript-eslint/eslint-plugin": "^5.45.0",
        "@typescript-eslint/parser": "^5.45.0",
        "clean-webpack-plugin": "^4.0.0",
        "copyfiles": "^2.4.1",
        "cpx": "^1.5.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.28.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-import-resolver-typescript": "^3.5.2",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-import-helpers": "^1.3.1",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-simple-import-sort": "^8.0.0",
        "eslint-plugin-sonarjs": "^0.16.0",
        "eslint-plugin-unicorn": "^45.0.1",
        "gh-pages": "^4.0.0",
        "husky": "^8.0.2",
        "jest": "^29.3.1",
        "lint-staged": "~13.0.4",
        "prettier": "^2.8.0",
        "release-it": "^15.5.0",
        "supertest": "^6.3.1",
        "ts-jest": "^29.0.3",
        "ts-loader": "^9.4.1",
        "ts-node": "^10.9.1",
        "ts-node-dev": "^2.0.0",
        "typeorm-extension": "^2.3.0",
        "typescript": "^4.9.3",
        "webpack": "^5.75.0",
        "webpack-cli": "^5.0.0",
        "webpack-node-externals": "^3.0.0"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".spec.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
